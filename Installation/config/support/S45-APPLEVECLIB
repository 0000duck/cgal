CXXFLAGS = -DCGAL_USE_F2C
LDFLAGS =  -framework vecLib
LIBS = f2c
REQUIRES =
INCOMPATIBLE = DOLLAR_WINDOWS_COMPILER
STDINCLDIRS =
INCLTHING =
STDLIBDIRS =
LIBTHING =
COMPILETESTFLAGS = 

