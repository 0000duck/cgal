#ifndef CGAL_LICENSE_CHECK_@PACKAGE_CODE_NAME@_H
#define CGAL_LICENSE_CHECK_@PACKAGE_CODE_NAME@_H

#include <CGAL/config.h>
#include <CGAL/license_check.h>
#include <CGAL/license_check/release_date.h>



#ifdef CGAL_@PACKAGE_CODE_NAME@_COMMERCIAL_LICENSE

#  if CGAL_@PACKAGE_CODE_NAME@_COMMERCIAL_LICENSE < CGAL_RELEASE_DATE

#    if defined(CGAL_LICENSE_WARNING)

#      ifdef BOOST_MSVC
#        pragma CGAL_WARNING(Your commercial license for CGAL does not cover this release of the @PACKAGE_PRETTY_NAME@ package)
#      else
#        pragma message( "WARNING: Your commercial license for CGAL does not cover this release of the @PACKAGE_PRETTY_NAME@ package") 
#      endif
#    endif

#    ifdef CGAL_LICENSE_ERROR
#      error Your commercial license for CGAL does not cover this release of the @PACKAGE_PRETTY_NAME@ package. You get this error, as you defined CGAL_LICENSE_ERROR.
#    endif // CGAL_LICENSE_ERROR

#  endif // CGAL_@PACKAGE_CODE_NAME@_COMMERCIAL_LICENSE < CGAL_RELEASE_DATE

#else // no CGAL_@PACKAGE_CODE_NAME@_COMMERCIAL_LICENSE

#  if defined(CGAL_LICENSE_WARNING)
#    ifdef BOOST_MSVC
#      pragma CGAL_WARNING(You use the CGAL @PACKAGE_PRETTY_NAME@ package under the terms of the GPLv3+)
#    else
#      pragma message("WARNING: You use the CGAL @PACKAGE_PRETTY_NAME@ package under the terms of the GPLv3+ ")
#    endif
#  endif // CGAL_LICENSE_WARNING

#  ifdef CGAL_LICENSE_ERROR
#    error ERROR: You use the CGAL @PACKAGE_PRETTY_NAME@ package under the terms of the GPLv3+. You get this error, as you defined CGAL_LICENSE_ERROR
#  endif // CGAL_LICENSE_ERROR


#endif // no CGAL_@PACKAGE_CODE_NAME@_COMMERCIAL_LICENSE

#endif // CGAL_LICENSE_CHECK_@PACKAGE_CODE_NAME@_H
