#!/bin/sh
cd $2/CGAL-3.2
QTDIR=/Developer/qt
DYLD_LIBRARY_PATH=/Developer/qt/lib
export QTDIR DYLD_LIBRARY_PATH
if [ -e upgrade.cgal ]; then
  rm -f upgrade.cgal
  exit 0
fi
touch MacOSX-install.log
./install_cgal -ni --CXX /usr/bin/g++ --CXXFLAGS "-Wno-long-long" >> $2/CGAL-3.2/MacOSX-install.log
exit 0
